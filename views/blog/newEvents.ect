<body>

<% include 'blog/navy.ect', {} %>

    <!-- Page Content -->
    <div class="container">

        <!-- Page Header -->
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">

               <% switch @classNumber : %>
                 <% when '1' : %>
                    高三十七班
                 <% end %>
                 <% when '2' : %>
                    兄弟连
                 <% end %>
                 <% when '3' : %>
                    初三八班
                 <% end %>
                 <% else : %>
                    系统错误
                  <% end %>
               <% end %>

                    <small>的神秘事件</small>
                </h1>
            </div>
        </div>
        <!-- /.row -->


        <div class="row" class="col-lg-12">
          <% for link in @resArr : %>

          <div class="row">
          <div  class="col-lg-12 col-sm-12">
            <div class="col-lg-2 col-lg-offset-0 col-sm-2  portfolio-item">
                <a href="#">
                    <img class="img-responsive img-circle" width="150" height="150" src="/images/headCapture/2.jpg" alt="">
                </a>
            </div>


            <div class="col-lg-1 col-sm-1 portfolio-item">
                <h3>


                    <% if link?.content.anonymity=="1" : %>
                                             <span>
                                                 <%= link.from.name %>
                                             </span>
                    <% else:%>
                                            <span>
                                                神秘人
                                            </span>
                    <% end %>



                </h3>

            </div>

            <div class="col-lg-6 col-sm-6 portfolio-item " >

                <h4 class="text-center">
                    <%= link.content.start %>
                </h4>

                <h3 class="text-center">
                        <%= link.content.content %>
                </h3>
                <h3 class="text-center">
                        <%= link.content.contentDetail %>
                </h3>

                <p>
                    <% if link?.content.targetname : %>
                         <img class="test-popup-link img-thumbnail" href="/timeLineupload/<%= link.content.targetName%>" src="/timeLineupload/<%= link.content.targetname%>">
                    <% end %>
                </p>
            </div>


            <div class="col-lg-1 col-sm-1 portfolio-item">
                <h3>

                 <% if link?.content.display=="1" : %>
                             <a href="/timeLine?_id=<%= link.__id %>">
                                                <%= link.name %>
                            </a>
                <% else:%>
                                        <span>
                                            神秘人
                                        </span>
                <% end %>



                </h3>

            </div>


            <div class="col-lg-2 col-lg-pull-0 col-sm-2 portfolio-item">
                <a href="#">
                    <img class="img-responsive img-circle" width="150" height="150" src="/images/headCapture/2.jpg" alt="">
                </a>
            </div>
          </div>

          </div>

          <hr/>

        <% end %>

        </div>



        <!-- Projects Row -->

        <!-- /.row -->


        <!-- Pagination -->
        <!--<div class="row text-center">
            <div class="col-lg-12">
                <ul class="pagination">
