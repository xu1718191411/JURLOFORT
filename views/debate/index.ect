socket.on("statementRefused",function(msg){

    $("#proStatement").html("")
    window.status = msg.status

    var proStatementCopyStatementRefused = $($("#proStatementCopyStatementRefused").html())
    $(proStatementCopyStatementRefused).find(".proStatementCopyStatementRefusedTxt").eq(0).text(msg.dealWithStatementResultTxt)
    $("#proStatement").append(proStatementCopyStatementRefused)

    $("#expandForm1").hide()
    $("#wait").hide()
    if(parseInt(window.status) == 0){
        if(parseInt($("#tachiba").attr("tachiba")) == 2){
                $("#waitTxt").text("肯定側の主張をお待ちください")
                $("#wait").show()
        }else{
            if(msg.LatestAnalysisMsg != 1){
                insertIntoProAnalysisWhenReceiveAnalysis(msg.LatestAnalysisMsg._obj,msg.LatestAnalysisMsg.dissentObj)
                _obj = msg.LatestAnalysisMsg._obj
                dissentObj = msg.LatestAnalysisMsg.dissentObj
            }else{
                _obj = []
                dissentObj = []
            }

            showNextStep()

        }
    }

    if(parseInt(window.status) == 2){
        if(parseInt($("#tachiba").attr("tachiba")) == 1){
                $("#waitTxt").text("否定側の主張をお待ちください")
                $("#wait").show()
        }else{
            if(msg.LatestAnalysisMsg != 1){
                insertIntoProAnalysisWhenReceiveAnalysis(msg.LatestAnalysisMsg._obj,msg.LatestAnalysisMsg.dissentObj)
                _obj = msg.LatestAnalysisMsg._obj
                dissentObj = msg.LatestAnalysisMsg.dissentObj
            }else{
                _obj = []
                dissentObj = []
            }

            showNextStep()

        }
    }
})


function finishDebate(){
                    swal("討論終了");
                    stopTimeCount()
                    $("#proAnalysis").html("討論終了")
                    $("#wait").hide();

                    socket.emit("endDebate",{})
                    return false;
}

function addViolateMessageToLeftTop(tachiba){
     if(tachiba == 1){
             // 0 pro 做陈述
            var sampleProState = $("#left").find(".top").eq(0).find(".proState").eq(0).prop('outerHTML');
            //先在左上角放入異議説明的说明文字
            $("#left").find(".bottom").eq(0).append(sampleProState)
            $("#left").find(".bottom").eq(0).find(".proState").eq(-1).find(".alert-link").eq(0).text("肯定側が時間を違反しました")
        }else{
            // 2 con 做陈述
            var sampleConState = $("#left").find(".top").eq(0).find(".conState").eq(0).prop('outerHTML');
            $("#left").find(".bottom").eq(0).append(sampleConState)
            $("#left").find(".bottom").eq(0).find(".conState").eq(-1).find(".alert-link").eq(0).text("否定側が時間を違反しました")
        }
}


function addViolateMessageToRightTop(tachiba){
    if(tachiba == 1){
        $($("#right").find("#rightContainer")[0]).append($($("#copy").find(".resultVPro")[0]).prop("outerHTML"))
    }else{
        $($("#right").find("#rightContainer")[0]).append($($("#copy").find(".resultVCon")[0]).prop("outerHTML"))
    }
}


